input ProductInput {
    name: String!,
    description: String,
    price: Float!,
    imgUrl: String!,
    categoryIdList: [ID]!
}

type Product {
    id: ID,
    name: String!,
    description: String,
    price: Float!,
    imgUrl: String!,
    status: String!,
    store: Store!,
    categories: [Category]!
}
